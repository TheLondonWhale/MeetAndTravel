<section class="main-block featured-wrap">
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-md-10">
                <div class="titile-block">
                    <h2>Les bons plans ajoutÃ©s rÃ©cemment</h2>
                    <br>
                </div>
            </div>
        </div>
        <div class="row">
          <% Tip.last(3).each do |tip| %>
            <div class="col-md-4 card-1">
                <div class="card">
                    <% if tip.pictures.attached? %>
                    <%= image_tag tip.pictures.first, class: "card-img-top"%>
                    <% else %>
                    <%= image_tag "featured-img2.jpg", class: "card-img-top"%>
                    <% end %>
                    <div class="card-body">
                        <h5 class="card-title"><%= tip.title %></h5>
                        <ul class="card-rating">
                            <li><%= tip.likes.count %><i class="fa fa-heart"></i></li>
                            <li><i class="fa fa-circle" aria-hidden="true"></i></li>
                            <li><%= tip.comments.count %> <%= tip.comments.count == 1 || 0 ? 'commentaire' : 'commentaires'%></li>
                            <li><i class="fa fa-circle" aria-hidden="true"></i></li>
                            <li>CatÃ©gorie xxx</li>
                        </ul>
                        <p class="card-text"><%= tip.description.truncate_words(25)%>
                          <% if tip.description.scan(/\w+/).size > 25 %>
                          <em><%= link_to "Lire plus", tip_path(tip.id)%></em>
                          <% end %>
                        </p>
                    </div>
                    <div class="card-bottom d-flex justify-content-between">
                        <p><i class="fa fa-location-arrow"></i>SAN FRANCISCO</p>
                        <p><%= link_to "Voir ce bon plan", tip_path(tip.id) %></p>
                    </div>

                </div>
            </div>
            <% end %>
        </div>
    </div>
</section>
